month = {oct},
number = {10},
pages = {1747--1754},
title = {{Sufici{\^{e}}ncia amostral para a estima{\c{c}}{\~{a}}o da produ{\c{c}}{\~{a}}o de cana-de-a{\c{c}}{\'{u}}car}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S0103-84782014001001747{\&}lng=pt{\&}tlng=pt},
