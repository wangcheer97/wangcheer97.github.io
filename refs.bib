title = {{Preparo das sementes de caf{\'{e}} para avalia{\c{c}}{\~{a}}o da viabilidade pelo teste de tetraz{\'{o}}lio}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S0101-31222011000100004{\&}lng=en{\&}nrm=iso{\&}tlng=pt http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S0101-31222011000100004{\&}lng=pt{\&}tlng=pt},
