@article{Carducci2012a,
abstract = {A resist{\^{e}}ncia do solo ao cisalhamento {\'{e}} um atributo que pode ser influenciado pela intensidade de uso e manejo do solo, resultando em significativas altera{\c{c}}{\~{o}}es nas propriedades f{\'{i}}sico-mec{\^{a}}nicas do mesmo. Este trabalho teve como objetivo avaliar a resist{\^{e}}ncia ao cisalhamento de um Latossolo Vermelho Distrof{\'{e}}rrico submetido a diferentes sistemas de uso e manejo, por meio de ensaio de palhetas com ‘Vane Shear Test' em laborat{\'{o}}rio. O estudo foi realizado em amostras com estrutura preservada, coletadas em {\'{a}}reas sob sistema plantio direto, pastagem e mata nativa, no munic{\'{i}}pio de Dourados-MS. O delineamento foi inteiramente casualizado em sistema fatorial (3 manejos x 2 profundidades x 7 repeti{\c{c}}{\~{o}}es), em cada uso foram tomadas amostras nas profundidades 0-0,05 e 0,05-0,10 m, as quais foram equilibradas ao potencial matricial de 6 kPa em c{\^{a}}mara de press{\~{a}}o de Richards. Em seguida foram submetidas ao ensaio de palhetas para a determina{\c{c}}{\~{a}}o do valor de resist{\^{e}}ncia ao cisalhamento e densidade do solo. Os maiores valores para resist{\^{e}}ncia ao cisalhamento e densidade do solo foram encontrados na {\'{a}}rea sob pastagem. A resist{\^{e}}ncia do solo ao cisalhamento esteve associada ao aumento da densidade do solo, uso e manejo, de forma distinta entre as profundidades estudadas.},
author = {Carducci, Carla Eloize and Vitorino, Antonio Carlos Tadeu and Bergamin, Anderson Cristian and Zeviani, Walmes Marques},
