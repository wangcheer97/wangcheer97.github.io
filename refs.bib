pages = {75--81},
title = {{Altera{\c{c}}{\~{o}}es f{\'{i}}sico-qu{\'{i}}micas e movimenta{\c{c}}{\~{a}}o de {\'{i}}ons em Latossolo gibbs{\'{i}}tico sob doses de gesso}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S0006-87052012000100012{\&}lng=en{\&}nrm=iso{\&}tlng=pt http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S0006-87052012000100012{\&}lng=pt{\&}tlng=pt},
