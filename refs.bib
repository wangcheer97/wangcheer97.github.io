}
@article{Clemente2011,
abstract = {O teste de tetraz{\'{o}}lio (TZ) {\'{e}} uma alternativa para avalia{\c{c}}{\~{a}}o r{\'{a}}pida da viabilidade de sementes j{\'{a}} adotada para a comercializa{\c{c}}{\~{a}}o de sementes de caf{\'{e}}. Um dos entraves para a utiliza{\c{c}}{\~{a}}o do teste {\'{e}} a dificuldade na extra{\c{c}}{\~{a}}o do endocarpo que demanda tempo e m{\~{a}}o de obra especializada. A utiliza{\c{c}}{\~{a}}o de hipoclorito de s{\'{o}}dio {\'{e}} recomendada para extra{\c{c}}{\~{a}}o do endocarpo de sementes de caf{\'{e}} para acelerar o processo de germina{\c{c}}{\~{a}}o, no entanto essa metodologia ainda n{\~{a}}o foi testada na realiza{\c{c}}{\~{a}}o do teste de tetraz{\'{o}}lio. Sementes de caf{\'{e}} de quatro lotes de duas cultivares foram submetidas a quatro metodologias para a realiza{\c{c}}{\~{a}}o do teste: extra{\c{c}}{\~{a}}o manual ou utiliza{\c{c}}{\~{a}}o de hipoclorito de s{\'{o}}dio (200 mL de NaClO a 5{\%}) para remo{\c{c}}{\~{a}}o do pergaminho e per{\'{i}}odos de embebi{\c{c}}{\~{a}}o das sementes em {\'{a}}gua (36 e 48 horas) para a retirada dos embri{\~{o}}es. Os resultados de viabilidade pelo TZ foram comparados com os de germina{\c{c}}{\~{a}}o das sementes com diferentes teores de {\'{a}}gua. O uso do NaClO na concentra{\c{c}}{\~{a}}o de 5{\%} por 6 horas de embebi{\c{c}}{\~{a}}o n{\~{a}}o {\'{e}} eficiente para a retirada do pergaminho e afeta negativamente os resultados do TZ em semente de caf{\'{e}} com teor de {\'{a}}gua abaixo de 25{\%}. O per{\'{i}}odo de embebi{\c{c}}{\~{a}}o de sementes de caf{\'{e}} em {\'{a}}gua por 48 horas na fase de preparo, para a realiza{\c{c}}{\~{a}}o do TZ, facilita a extra{\c{c}}{\~{a}}o dos embri{\~{o}}es, n{\~{a}}o afetando os resultados do teste.},
