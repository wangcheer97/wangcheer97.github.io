title = {{Modelagem da curva de reten{\c{c}}{\~{a}}o de {\'{a}}gua de Latossolos utilizando a Equa{\c{c}}{\~{a}}o Duplo Van Genuchten}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S0100-06832011000100007{\&}lng=en{\&}nrm=iso{\&}tlng=pt http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S0100-06832011000100007{\&}lng=pt{\&}tlng=pt},
