@article{Silva2019,
abstract = {Ovos, juvenis de segundo est{\'{a}}dio (J2) e f{\^{e}}meas de Meloidogyne incognita foram inseridos em solu{\c{c}}{\~{a}}o contendo o princ{\'{i}}pio ativo aldicarbe com a{\c{c}}{\~{a}}o nematicida. A exposi{\c{c}}{\~{a}}o ao nematicida ocasionou: (1) redu{\c{c}}{\~{a}}o na eclos{\~{a}}o, (2) redu{\c{c}}{\~{a}}o na mobilidade, (3) mortalidade de J2, e (4) redu{\c{c}}{\~{a}}o na penetra{\c{c}}{\~{a}}o. A aplica{\c{c}}{\~{a}}o do nematicida ap{\'{o}}s a infec{\c{c}}{\~{a}}o com J2 saud{\'{a}}veis foi mais eficiente em reduzir a reprodu{\c{c}}{\~{a}}o do nematoide. A aplica{\c{c}}{\~{a}}o feita mais pr{\'{o}}xima da inocula{\c{c}}{\~{a}}o causou maiores redu{\c{c}}{\~{o}}es de galhas e de ovos/g de raiz de planta de soja, com maior efeito na reprodu{\c{c}}{\~{a}}o. Enquanto os J2 incubados por 24 h em aldicarbe 50 $\mu$g mL-1 e inoculados em soja tiveram o n{\'{u}}mero de ovos/g raiz reduzido em 2,5{\%}, em compara{\c{c}}{\~{a}}o {\`{a}} testemunha, a aplica{\c{c}}{\~{a}}o da mesma concentra{\c{c}}{\~{a}}o ap{\'{o}}s a inocula{\c{c}}{\~{a}}o resultou em 95{\%} de redu{\c{c}}{\~{a}}o. A irriga{\c{c}}{\~{a}}o do substrato com solu{\c{c}}{\~{a}}o de aldicarbe impediu a penetra{\c{c}}{\~{a}}o e interrompeu o ciclo. O teor de energia lip{\'{i}}dica corporal dos J2 foi avaliado por colora{\c{c}}{\~{a}}o com “Oil Red O”, e as fotografias analisadas por programa de an{\'{a}}lise de imagem. A energia corporal foi semelhante nos J2 incubados em aldicarbe e em {\'{a}}gua pura a 25 {\textordmasculine}C at{\'{e}} oito dias. Portanto, o aldicarbe afeta aditivamente as v{\'{a}}rias fases do ciclo de vida de M. inc{\'{o}}gnita, com maior efeito na reprodu{\c{c}}{\~{a}}o e na desorienta{\c{c}}{\~{a}}o dos J2 para as ra{\'{i}}zes de soja, sem preservar a energia corporal dos J2 im{\'{o}}veis.},
author = {da Silva, Esdras Henrique and Campos, Vicente Paulo and Rocha, Fernando Da Silva and Zeviani, Walmes Marques and Terra, Willian C{\'{e}}sar},
doi = {10.32404/rean.v6i3.2164},
